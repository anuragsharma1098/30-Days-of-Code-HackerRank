#include <bits/stdc++.h>
using namespace std;
void o(int n)
    {
        if(n>=2 && n<=5)
        {cout<<"Not Weird";}
    if(n>=6 && n<=20)
    {cout<<"Weird";}
        if(n>20)
        {cout<<"Not Weird";
    }
        }
  
int main()
{
int n;
cin>>n;
if(n%2==0)
{
o(n);
}
else {
    cout<<"Weird";
}
return 0;
}
